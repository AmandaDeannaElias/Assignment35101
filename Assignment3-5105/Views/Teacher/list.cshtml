@model IEnumerable<Assignment3_5105.Models.Teacher>

@{
    ViewBag.Title = "list";
}
<nav>
    <a class="navlink" href="/Teacher/New">New Teacher</a>
</nav>
<h2>Full List of Teachers</h2>

@foreach (var teacher in Model)
{
    <div><a href="/Teacher/Show/@teacher.teacherId"> @teacher.teacherfname @teacher.teacherlname @teacher.teacherId @teacher.salary @teacher.hiredate</a></div>
}
